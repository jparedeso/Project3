<div id="main" class='container formContainer'>
  <div class="row">
    <div class="col-sm-12">
      <h1 class="flash" id="regReq">WAIT... THE PRINCESS REQUESTS THAT YOU REGISTER OR LOGIN BEFORE ENTERING THE KINGDOM!
      </h1>
      <br>
    </div>
  </div>
  <div id="logInForm" class="row">
      <form asp-route-returnurl="@ViewData["ReturnUrl"]" method="post">
      <div class="form-group">
        <label class="rounded">EMAIL ADDRESS</label>
        <input type="email" class="noRadius rounded form-control" aria-describedby="emailHelp" placeholder="EXAMPLE@EMAIL.MAIL">
      </div>
      <div class="form-group">
        <label class="rounded">PASSWORD</label>
        <input type="password" class="noRadius form-control rounded" placeholder="PASSWORD"></input>
      </div>
      <button type="submit" class="btn btn-success" id='submitButton'>LOGIN</button>
      <br>
      <br>
      <a class="registrationLink">DON'T HAVE AN ACCOUNT? CREATE ONE HERE</a>
      <br>
    </form>
  </div>
</div>
<br>
